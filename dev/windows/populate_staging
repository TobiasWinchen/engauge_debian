FROMLIBDIR="/cygdrive/C/QtOpenSource/Qt5.5.1/5.5/mingw492_32/lib/"
echo "Copying libraries from $FROMLIBDIR"
cp $FROMLIBDIR/libQt5Core.a         staging/libQt5Core551.a
cp $FROMLIBDIR/libQt5Gui.a          staging/libQt5Gui551.a
cp $FROMLIBDIR/libQt5Help.a         staging/libQt5Help551.a
cp $FROMLIBDIR/libQt5Network.a      staging/libQt5Network551.a
cp $FROMLIBDIR/libQt5PrintSupport.a staging/libQt5PrintSupport551.a
cp $FROMLIBDIR/libQt5Widgets.a      staging/libQt5Widgets551.a
cp $FROMLIBDIR/libQt5Xml.a          staging/libQt5Xml551.a

export FROMEXEDIR="$HOME/engauge6/bin/"
echo "Copy executable from $FROMEXEDIR"
cp $FROMEXEDIR/engauge.exe          staging
